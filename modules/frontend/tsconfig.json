{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": "src",
    "rootDir": "src",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "isolatedModules": true
  },
  "include": [
    "src"
  ]
}