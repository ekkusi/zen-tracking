{
  "name": "@ekeukko/zen-tracking-frontend",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@chakra-ui/icons": "^1.0.5",
    "@chakra-ui/react": "^1.1.2",
    "@ekeukko/zen-tracking-backend": "^1.0.0",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "date-fns": "^2.17.0",
    "formik": "^2.2.6",
    "framer-motion": "^3.10.0",
    "logrocket": "^1.0.14",
    "react": "^17.0.1",
    "react-calendar": "^3.3.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-select": "^4.2.1",
    "styled-components": "^5.2.1",
    "use-global-hook": "^0.2.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "concurrently -n react,apollo-codegen \"yarn start-react\" \"yarn apollo:update-types-watch\"",
    "start-react": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "stop": "kill -9 $(lsof -t -i:3000)",
    "gen:theme-typings": "chakra-cli tokens ./src/theme.ts --out=../../node_modules/@chakra-ui/theme/dist/types/custom-theme.d.ts",
    "apollo:update-types": "apollo client:codegen --target typescript --globalTypesFile src/__generated__/globalTypes.ts",
    "apollo:update-types-watch": "yarn apollo:update-types --watch",
    "apollo:update-types-clean": "find . -type d -name __generated__ -prune -exec rm -rf {} \\; &&  yarn apollo:update-types && rm -rf __generated__"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.2.0",
    "@types/date-fns": "^2.6.0",
    "@types/react-calendar": "^3.1.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.13",
    "@types/styled-components": "^5.1.7",
    "@types/use-global-hook": "^0.1.3",
    "apollo": "^2.32.5"
  }
}
