{
  "name": "@ekkusi/zen-tracking-backend",
  "version": "1.0.0",
  "main": "lib/index",
  "typings": "lib/types/index",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/import-types-preset": "^1.18.1",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-resolvers": "^1.18.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express-fileupload": "^1.1.6",
    "@types/textract": "^2.4.0",
    "@types/uuid": "^8.3.0",
    "copyfiles": "^2.4.1",
    "kill-port": "^1.6.1",
    "nodemon": "^2.0.7",
    "prisma": "^2.18.0",
    "shx": "^0.3.3",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "@prisma/client": "^2.18.0",
    "@types/lodash.merge": "^4.6.6",
    "apollo-server-errors": "^2.4.2",
    "apollo-server-express": "^2.19.1",
    "aws-sdk": "^2.863.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "date-fns": "^2.18.0",
    "dot-env": "^0.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "graphql": "^15.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.merge": "^4.6.2",
    "textract": "^2.5.0",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development concurrently -n server,graphql-codegen --kill-others \"nodemon -V\"  \"yarn generate-watch\"",
    "start:server": "NODE_ENV=development nodemon -V",
    "build": "shx rm -rf lib && cross-env NODE_ENV=production tsc -p tsconfig-prod.json && copyfiles -u 1 \"src/**/*.graphql\" \"src/**/*.json\" \"src/**/*.d.ts\" lib",
    "prod": "cross-env NODE_ENV=production node lib/app.js",
    "generate-types": "graphql-codegen --config codegen.yml && eslint src/types/* --ext .ts --fix",
    "generate-watch": "graphql-codegen --config codegen.yml --watch",
    "gen-and-build:": "yarn generate-types && yarn build",
    "prisma-introspect": "yarn prisma introspect",
    "prisma-generate": "yarn prisma generate",
    "prisma-migrate": "prisma migrate dev --preview-feature",
    "update-prisma": "yarn prisma-introspect && yarn prisma-generate",
    "init-mock-data": "ts-node src/mock/initMockData.ts",
    "stop": "kill -9 $(lsof -t -i:4000)",
    "generate-quotes": "ts-node ./src/utils/extractText.ts"
  }
}
